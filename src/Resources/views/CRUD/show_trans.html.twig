{% extends '@NetworkingInitCms/CRUD/base_show_field.html.twig' %}

{% block field%}
    {% if field_description.options.translation_domain is not defined %}
        {% set value = value|trans %}
    {% else %}
        {% set value = value|trans({}, field_description.options.translation_domain) %}
    {% endif %}

    {% if field_description.options.safe %}
        {{ value|raw }}
    {% else %}
        {{ value }}
    {% endif %}
{% endblock %}