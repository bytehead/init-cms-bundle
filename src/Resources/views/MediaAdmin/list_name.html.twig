{% extends '@NetworkingInitCms/CRUD/base_list_field.html.twig' %}

{% block field %}
    <div>
        <a href="{{ admin.generateUrl('edit', {'id' : object.id}) }}" style="float: left; margin-right: 6px;">{% media object, 'admin' with {'width': 75, 'height': 60} %}</a>
        <a href="{{ admin.generateUrl('edit', {'id' : object.id}) }}">{{ object.name }}</a>
        {% if object.width is defined and object.width %}
        <br />
        <span class="info-soft">{{ object.providerName|trans({}) }}: {{ object.width }}x{{ object.height }}</span> <br />
        {% endif %}
    </div>
{% endblock %}